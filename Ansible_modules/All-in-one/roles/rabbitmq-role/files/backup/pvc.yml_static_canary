apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: data-rabbitmq-0
  namespace: rabbitmq
spec:
  storageClassName: canary-manual
  accessModes:
    - ReadWriteMany
  resources:
    requests:
       storage: 1Gi
---

